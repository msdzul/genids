      "api_hash": {
        "description": "Get this value from my.telegram.org.",
        "required": true,
        "value": ""
      },
      "token": {
        "description": "Obtain a Telegram bot token by contacting @BotFather",
        "required": true,
        "value": ""
      },
      "MONGO_URI": {
        "description": "Database mongo db uri",
        "required": true,
        "value": ""
      },
      "DB_NAME": {
        "description": "Database mongo db name",
        "required": true,
        "value": "genids_bot"
      }
    }
